.StockCheckList_head{
	overflow: hidden;
}
.StockCheckList_head_bl{
	width: 50%;
	float: left;
	display: -webkit-box;
	display: -webkit-flex;
	display: -ms-flexbox;
	display: flex;
	-webkit-box-align: center;
	-webkit-align-items: center;
	    -ms-flex-align: center;
	        align-items: center;
	-webkit-box-pack: center;
	-webkit-justify-content: center;
	    -ms-flex-pack: center;
	        justify-content: center;
	height: 60rpx;
	font-size: 28rpx;
}
.StockCheckList_head_bl text:nth-child(1){
	width: 150rpx;
	text-align: right;
}
.StockCheckList_head_bl text:nth-child(2){
	width: 200rpx;
	text-align: center;
}
.StockCheckList_body_list{
	border-bottom: 1rpx solid #DDDDDD;
	font-size: 22rpx;
}
.StockCheckList_body_listA{
	display: -webkit-box;
	display: -webkit-flex;
	display: -ms-flexbox;
	display: flex;
	-webkit-box-align: center;
	-webkit-align-items: center;
	    -ms-flex-align: center;
	        align-items: center;
	height: 80rpx;
}
.StockCheckList_body_listA>text:nth-child(1){
	margin-left: 20rpx;
}
.StockCheckList_body_listA>view:nth-child(2){
	margin-left: 20rpx;
}
.StockCheckList_body_listA>view:nth-child(3){
	margin-left: 20rpx;
}
.StockCheckList_body_listA>text:nth-child(4){
	margin-left: 40rpx;
}
.StockCheckList_body_listB{
	display: -webkit-box;
	display: -webkit-flex;
	display: -ms-flexbox;
	display: flex;
	-webkit-box-orient: vertical;
	-webkit-box-direction: normal;
	-webkit-flex-direction: column;
	    -ms-flex-direction: column;
	        flex-direction: column;
	padding-left: 100rpx;
}

